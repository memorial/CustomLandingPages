		<script type="text/javascript">

			$(document).ready(function () {


				$('.input-phone').intlInputPhone();

				var isFilterOn = false;
				$(document).on("input", "#phone2", function () {
					if (isFilterOn)
						this.value = this.value.replace(/\D/g, '');
				});

				$('#phone2').mask('(500) 000-00-00', { placeholder: "(5__) ___-__-__" });

				$('body').on('DOMSubtreeModified', '.btn-cc', function () {
					if ($(".btn-cc").html().replace(/&nbsp;/g, '') != "+90") {
						$('#phone2').unmask();
						$('#phone2').attr("maxlength", "15");
						isFilterOn = true;
					}
					else {
						$('#phone2').mask('(500) 000-00-00', { placeholder: "(5__) ___-__-__" });
						isFilterOn = false;
					}
				});


				$("#askprice_permission_button_send").click(function () {
					var nErrCount = 0;
					var strErr = "";
					if ($("#countries_phone2").val() == "") {
						$("#ask_price_errorID").show();
						strErr += 'Lütfen ülke kodunu seçiniz<br>';
						nErrCount += 1;
					}


					if ($("#fullname").val() == "") {
						strErr += 'Lütfen ad ve soyad giriniz.<br>';
						nErrCount += 1;
					}

					if ($("#phone2").val().length < 10) {
						strErr += 'Telefon 10 haneden küçük olamaz<br>';
						nErrCount += 1;
					}
					var regex = /^[a-zA-Z0-9.!#$%&'*+/=?^_`{|}~-]+@[a-zA-Z0-9-]+(?:\.[a-zA-Z0-9-]+)*$/;
					if (!regex.test($("#email").val())) {
						strErr += 'Geçerli bir Emaili giriniz.<br>';
						nErrCount += 1;
					}

					if (!$("#askprice_permission").is(":checked")) {
						strErr += 'KVKK Metnini onaylayınız.<br>';
						nErrCount += 1;
					}

					if (nErrCount == 0) {
						$("#spErr").html("");
						$("#askprice_permission_button_send").attr("disabled", "disabled");
						$('#askprice_permission_button_send').html('Gönderiliyor...');

						tel = $(".btn-cc").html().replace(/&nbsp;/g, '') + " " + $("#phone2").val().replace(/[^0-9]+/g, "");
						$('#phone').val(tel);
						send_promotion();
					}
					else
						$("#spErr").html("<span style='color:#b9194b;'>" + strErr + "</span>");

				});





			});
			function reactivateForm() {
				document.getElementById("askprice_form").reset();
				$("#askprice_form").show();
				$(".alert-success").hide();
			}

			//function f1() {
			//    const queryString = window.location.search;
			//    kurumsallink.href = kurumsal + queryString;
			//}

			//function f2() {
			//    const queryString = window.location.search;
			//    bireysellink.href = bireysel + queryString;
			//}

			//url: '/landingContactPost?lang=tr&type=10',
			function send_promotion() {


				$(".alert-success").hide();
				$(".alert-warning").hide();
				$.ajax({					
                    url: '/landingAskPricePost?lang=tr',
					type: "POST",
					async: true,
					data: $("#askprice_form").serialize(),
					cache: false,
					beforeSend: function () {
						$('#askprice_permission_button_send').prop('disabled', true);
						$('#askprice_permission_button_send').html('Gönderiliyor...');
					},
					success: function (b) {
						if (b == "1" || (typeof b.status !== 'undefined' && b.status == "1")) {
							$('#askprice_form input:not([type=hidden]), #askprice_form textarea').val('');
							$("#fullname").val("");
							$("#phone2").val("");
							$('#askprice_permission_button_send').prop('disabled', false);
							$('#askprice_permission_button_send').html('Gönder');
							$("#spErr").html("<span style='margin: 20px 0; display: block; color: #25a517;'>Formunuz başarıyla gönderilmiştir.</span>");
							$("html, body").animate({ scrollTop: $(document).height() - 900 }, 100);
							
							dataLayer.push({
								'event': 'landing_finishForm',
								'formName': window.gtmFormName, 
								'pageCategory':'Landing Page'
							});

                            if (typeof b.hashedPhoneNumber !== 'undefined') {

                                dataLayer.push({
                                    'event': 'create_lead_user',
                                    'userID': b.hashedPhoneNumber
                                });
                            }
							
						} else {
							$(".alert-warning").fadeIn();
							$('#askprice_permission_button_send').html('Gönder');
							$("#spErr").html("<span style='margin: 20px 0; display: block; color: #ca2020;'>Teknik bir problem oldu. Lütfen daha sonra tekrar deneyiniz.</span>");
						}
						$('#askprice_permission_button_send').prop('disabled', false);
					},
					error: function (b, d, c) {
						$(".alert-warning").fadeIn();
						$('#askprice_permission_button_send').html('Gönder');
						$('#askprice_permission_button_send').prop('disabled', false);
					}
				});
			}

			$(".accordion-title").on('click', function (event) {
				if ($(this).parent().find(".accordion-content").hasClass("show"))
					$(this).parent().find(".accordion-content").removeClass("show");
				else
					$(this).parent().find(".accordion-content").addClass("show");
			});

			$(".sss").on('click', function (e) {
				$(".meanclose")[0].click();
			});

		</script>
		
